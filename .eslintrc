{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "react/jsx-indent": 0,
    "comma-dangle": 0,
    "no-sequences": 0,
    "arrow-body-style": 0,
    "no-mixed-spaces-and-tabs": 0,
    "indent": 0,
    "no-unused-expressions": ["error", { "allowShortCircuit": true }],
    "no-shadow": 0
  }
}
